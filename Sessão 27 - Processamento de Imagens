iterator:\n","        dataframe_batch[\"data_as_resized_array\"] = dataframe_batch.apply(resize_img, args=(True,), axis=1)\n","        dataframe_batch[\"data_as_array\"] = dataframe_batch.apply(resize_img, args=(False,), axis=1)\n","        yield dataframe_batch"],"metadata":{"id":"yC0sImFnugDZ"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["resized_df = images_df.select(\"image.*\").mapInPandas(resize_image_udf, schema)\n","resized_df"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"JYSF5_eCu1rX","executionInfo":{"status":"ok","timestamp":1688689715003,"user_tz":180,"elapsed":5,"user":{"displayName":"Luan Corumba","userId":"07892592111474770617"}},"outputId":"eaecf85d-ed6c-4b11-8b35-4de73e5084a4"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["DataFrame[origin: string, height: int, width: int, nChannels: int, mode: int, data: binary, data_as_resized_array: array<int>, data_as_array: array<int>]"]},"metadata":{},"execution_count":43}]},{"cell_type":"code","source":["row = resized_df.take(1)[0]\n","\n","Image.frombytes(mode='RGB', data=bytes(row.data_as_array), size=[row.width,row.height]).show()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":49},"id":"q1sbV4B-vCIm","executionInfo":{"status":"ok","timestamp":1688689744718,"user_tz":180,"elapsed":3076,"user":{"displayName":"Luan Corumba","userId":"07892592111474770617"}},"outputId":"2bae0fd2-864d-43b8-dd80-db910e6fac47"},"execution_count":null,"outputs":[{"output_type":"display_data","data":{"text/plain":["<PIL.Image.Image image mode=RGB size=32x32 at 0x7FF7D85AA3B0>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"code","source":["Image.frombytes(mode='RGB', data=bytes(row.data_as_resized_array), size=[224,224]).show()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":241},"id":"hru5oy07vNio","executionInfo":{"status":"ok","timestamp":1688689830681,"user_tz":180,"elapsed":9,"user":{"displayName