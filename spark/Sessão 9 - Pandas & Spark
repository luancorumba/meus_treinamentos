},{"cell_type":"code","source":["from pyspark.sql.functions import pandas_udf, PandasUDFType\n","from pyspark.sql.types import DoubleType"],"metadata":{"id":"9NMnvsRpIGUP"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# Defina a função Python que implementa a lógica desejada\n","def sqrt_pandas(series: ps.Series) -> ps.Series:\n","    return series ** 0.5\n","\n","# Registre a função Python como uma UDF vetorial\n","sqrt_pandas_udf = pandas_udf(sqrt_pandas, returnType=DoubleType(), functionType=PandasUDFType.SCALAR)\n","\n","# Aplique a UDF vetorial registrada aos dados do DataFrame\n","airbnb_rj = airbnb_rj.to_spark(index_col=\"id\")\n","df_with_udf = airbnb_rj.select(sqrt_pandas_udf(airbnb_rj[\"price\"]).alias(\"Square_Root\"))\n","df_with_udf.show()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"y5mcLZwGIdGG","executionInfo":{"status":"ok","timestamp":1685928223522,"user_tz":180,"elapsed":2256,"user":{"displayName":"Luan Corumba","userId":"07892592111474770617"}},"outputId":"638bde1e-bc4e-4460-94b2-bb0c675d3d5d"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stderr","text":["/content/spark-3.4.0-bin-hadoop3/python/pyspark/sql/pandas/functions.py:399: UserWarning:\n","\n","In Python 3.6+ and Spark 3.0+, it is preferred to specify type hints for pandas UDF instead of specifying pandas UDF type which will be deprecated in the future releases. See SPARK-28264 for more details.\n","\n"]},{"output_type":"stream","name":"stdout","text":["+------------------+\n","|       Square_Root|\n","+------------------+\n","|18.708286933869708|\n","|24.979991993593593|\n","|              10.0|\n","|15.362291495737216|\n","| 17.52141546793523|\n","| 16.30950643030009|\n","|13.416407864998739|\n","|47.644516998286385|\n","| 43.57751713900185|\n","|39.698866482558415|\n","|20.663978319771825|\n","|  23.2163735324878|\n","| 25.03996805109783|\n","|18.439088914585774|\n","|14.628738838327793|\n","|14.142135623730951|\n","|13.